#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/romarioburke/Documents/Flyde/flyde/node_modules/.pnpm/knip@5.61.3_@types+node@20.19.4_typescript@5.8.3/node_modules/knip/bin/node_modules:/Users/romarioburke/Documents/Flyde/flyde/node_modules/.pnpm/knip@5.61.3_@types+node@20.19.4_typescript@5.8.3/node_modules/knip/node_modules:/Users/romarioburke/Documents/Flyde/flyde/node_modules/.pnpm/knip@5.61.3_@types+node@20.19.4_typescript@5.8.3/node_modules:/Users/romarioburke/Documents/Flyde/flyde/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/romarioburke/Documents/Flyde/flyde/node_modules/.pnpm/knip@5.61.3_@types+node@20.19.4_typescript@5.8.3/node_modules/knip/bin/node_modules:/Users/romarioburke/Documents/Flyde/flyde/node_modules/.pnpm/knip@5.61.3_@types+node@20.19.4_typescript@5.8.3/node_modules/knip/node_modules:/Users/romarioburke/Documents/Flyde/flyde/node_modules/.pnpm/knip@5.61.3_@types+node@20.19.4_typescript@5.8.3/node_modules:/Users/romarioburke/Documents/Flyde/flyde/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../knip/bin/knip.js" "$@"
else
  exec node  "$basedir/../knip/bin/knip.js" "$@"
fi
